import java.util.Scanner;
public class energetico {

	public static void main(String[] args) {
	    Scanner sc = new Scanner(System.in);
        System.out.println("********** ENERGÉTICOS ACELERATOR **********");
	    System.out.println("Cliente: ");
	    String cliente = sc.next();
	    System.out.println("Quantidade de Energéticos: ");
	    int quantidade = sc.nextInt();
	    double valor_compra = quantidade * 4.50;
	    double icms = valor_compra * 0.18;
	    double ipi = valor_compra * 0.04;
	    double pis = valor_compra * 0.0186;
	    double cofins = valor_compra * 0.0854;
	    double total_geral = valor_compra + icms + ipi + pis + cofins;
	    System.out.printf("Icms: %.2f%n", icms);
	    System.out.printf("Ipi: %.2f%n", ipi);
	    System.out.printf("Pis: %.2f%n", pis);
	    System.out.printf("Cofins: %.2f%n", cofins);
	    System.out.printf("O total da compra de %s foi de %.2f.", cliente, total_geral);

	sc.close();
	}

}